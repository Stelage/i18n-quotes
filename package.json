{
	"name": "@victr/i18n-quotes",
	"version": "4.0.0",
	"description": "Retrieves random quotes with different languages",
	"keywords": [
		"quotes",
		"i18n"
	],
	"author": "Victor Azevedo",
	"license": "ISC",
	"type": "module",
	"scripts": {
		"cloudflare:dev": "pnpm --filter cloudflare dev",
		"cloudflare:deploy": "pnpm --filter cloudflare deploy",
		"netlify:dev": "netlify dev --filter netlify --framework #static"
	},
	"devDependencies": {
		"vite": "^5.0.8",
		"vitest": "^1.0.4"
	},
	"prettier": {
		"tabWidth": 4,
		"semi": false,
		"useTabs": true,
		"printWidth": 120,
		"singleQuote": true
	}
}
